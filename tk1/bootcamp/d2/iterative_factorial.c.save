#include <unistd.h>
#include <stdlib.h>
#include <stdio.h>

int iterative_factorial(int nb)
{
    int res = 1;
    for(int i = 1;i <= nb;i++)
        res *= i;
    return res;
}

int main(int argc, char **argv)
{
    printf("result: %d", iterative_factorial(argv[1])):
    return 0;
}
