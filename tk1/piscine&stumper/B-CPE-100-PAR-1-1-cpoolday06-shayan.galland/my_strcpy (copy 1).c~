#include <unistd.h>

int my_putchar(char c)
{
    write(1, &c, 1);
}    

char *my_strcpy(char *dest, char const *src)
{
    int b = 0; 	
    
    while(src[b] != '\0'){
    dest[b];
    my_putchar(dest[b]);    
    b ++;
    }
}
void my_putstr(char *src)
{
    int i;

    i = 0;
    while(src[i] != '\0'){
        my_putchar(src[i]);
        i = i + 1;
    }
}
int main(void)
{
    char source[] = "jour";
   
    my_strcpy(dest, source);
    my_putstr(dest);
    
    return(0);
}
