#include "functions.h"

void print_star (char size)
{
    my_putchar(size);
}


int main (int argc, char **argv)
{
    if (argc == 1)
        return 0;
    if (argc > 2)
        return 84;
    if (argv[1][0] != '-')
        return 84;
    if (argv[1][1] <= '9'&& argv[1][1] >= '0')
        print_star(argv[1][1]);
    else
        return 84;
    return 0;
}
