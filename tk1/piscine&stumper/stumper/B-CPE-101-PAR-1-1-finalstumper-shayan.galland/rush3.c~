#include <unistd.h>
#include <stdlib.h>
#include <string.h>
#include "rush-1-1.c"
#include "rush-1-2.c"
#include "rush-1-3.c"
#include "rush-1-4.c"
#include "rush-1-5.c"

int my_strlen(char const *s)
{
    int i = 0;
    while(s[i] != '\0'){
	i++;
    }
    return(i);
}

char **rush3P1(char *str)
{
    int y = 0;
    int x = 0;
    int r = 0;
    char **tabl = malloc(sizeof(char) * strlen(str));
    
    tabl[y][x] = str[r];
    
    while(tabl[y][x] != '\0'){
        while(tabl[y][x] != '\n'){
            if(tabl[0][x] == (x * '-' - 2))
		return(rush-1-1.c);
            if(tabl[0][x] == (x * '*' - 2))
		return(rush-1-2.c);
            if(tabl[0][x] == 'A')
		return(rush-1-3.c);
            x++;
            r++;
	}
        x = 0;
	y++;
    }
}

char **rush3P2(char *str)
{
    int l = 1;
    int n = 0;
    int p = 0;
    
    char **tabl = malloc(sizeof(char) * strlen(str));

    tabl[l][n] = str[p]
    
    while(tabl[l][n] != '\0'){
        while(tabl[l][n] != '\n'){
            if(tabl[l][0] == 'A')
                return(rush-1-4.c);
            if(tabl[l][n] == 'C')
                return(rush-1-5.c);
            n++;
            p++;
        }
        n = 0;
        l++;
    }
}
