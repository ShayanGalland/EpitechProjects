#include <unistd.h>
#include <stdlib.h>
#include <string.h>

int my_strlen(char const *s)
{
    int i = 0;
    while(s[i] != '\0')
        i++;
    return(i);
}
int step1(char* const letter)
{
    for(letter == i)
        my_putchar(i);
}

void  my_putchar(char c)
{
    write(1 ,&c ,1);
}

void my_putstr(char *src)
{
    int i;

    i = 0;
    while(src[i] != '\0'){
        my_putchar(src[i]);
        i = i + 1;
    }
}
int main(void)
{
    my_putstr(my_strlen(step1("J'aime bien les bateau")));

    return(0);
}
